<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todolistappforwillingsgalk">

    <application
        android:name=".presentation.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/todoicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/todoicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToDoListAppforWillingsGalk">
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ToDoListAppforWillingsGalk">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>